{
  "name": "alexandrmazur/apilayer-coinlayer-php",
  "description": "PHP SDK for Apilayer Coinlayer product",
  "keywords": [
    "apilayer",
    "apilayer client",
    "coinlyaer",
    "coinlyaer client",
    "coinlyaer sdk",
    "coinlyaer php",
    "coinlyaer php sdk",
    "apilayer coinlyaer sdk"
  ],
  "type": "library",
  "license": ["MIT"],
  "authors": [
    {
      "name": "Olexandr Mazur",
      "email": "alexandrmazur96@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "psr/http-client": "1.*",
    "psr/http-factory": "1.*",
    "psr/http-message": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "php-http/guzzle6-adapter": "^2.0",
    "guzzlehttp/psr7": "1.7.*",
    "laminas/laminas-diactoros": "2.4.1",
    "php-http/discovery": "1.12.*",
    "phpspec/prophecy-phpunit": "2.*",
    "vimeo/psalm": "4.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Apilayer\\Coinlayer\\": "src/",
      "Apilayer\\Tests\\": "tests/"
    }
  }
}
